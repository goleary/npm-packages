{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noEmit": true,
        "lib": ["esnext", "dom"],
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "noUncheckedIndexedAccess": true,
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true
    }
}
